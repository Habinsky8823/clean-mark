{
  "authors": [
    "Cristi Constantin <cristi.constantin@live.com>"
  ],
  "bin": {
    "clean-mark": "./bin/clean-mark"
  },
  "dependencies": {
    "a-extractor": "git+https://git@github.com/croqaz/a-extractor.git",
    "breakdance": "3.x",
    "cheerio": "^1.0.0-rc.2",
    "chrono-node": "1.x",
    "minimist": "1.x",
    "minimize": "2.x",
    "node-fetch": "^2.0.0-alpha.9",
    "sanitize-html": "1.16.x",
    "string": "3.x"
  },
  "description": "Convert an article into a clean Markdown",
  "devDependencies": {
    "micro": "9.x",
    "mocha": "4.x",
    "prettier": "1.9.2",
    "should": "13.x",
    "standard": "10.0.3"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "cli",
    "clearly",
    "readability",
    "markdown"
  ],
  "license": "MIT",
  "main": "lib/clean.js",
  "name": "clean-mark",
  "overrides": [
    {
      "files": "lib/*.js",
      "options": {
        "printWidth": 120,
        "semi": false,
        "singleQuote": true
      }
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/croqaz/clean-mark.git"
  },
  "scripts": {
    "format": "npx prettier --write lib/*.js && npx standard --fix lib/**.js",
    "lint": "npx standard lib/**.js",
    "start": "micro server.js",
    "test": "npx mocha $(find . -name './test/*.test.js')"
  },
  "version": "0.4.4"
}
